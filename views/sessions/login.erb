<h1>Login</h1>
<%= error_message%>
<form action="/sessions" method="POST">
    <div>
      <label for="username">
        Username:
      </label>
      <input type="text" name="username" placeholder="Enter Username"/>
    </div>

    <div>
    <label for="password">
      Password:
    </label>
    <input type="password" name="password" placeholder="Enter password"/>
    </div>

    <input type="submit" value="Login!" />
</form>